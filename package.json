{
  "name": "dom-node-export",
  "version": "1.0.2",
  "description": "A package for exporting the DOM element to a separate xhtml document.",
  "author": "Yegor Pelykh <yegor.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yegor-pelykh/dom-node-export"
  },
  "keywords": [
    "dom",
    "node",
    "export",
    "xhtml",
    "html",
    "document",
    "computed"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    "lib/**/*d.ts"
  ],
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --no-error-on-unmatched-pattern",
    "lint": "tslint -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
